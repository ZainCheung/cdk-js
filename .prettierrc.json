{
  "plugins": [
    "prettier-plugin-organize-imports"
  ],
  "singleQuote": true,
  "semi": true,
  "printWidth": 80,
  "tabWidth": 4,
  "trailingComma": "es5",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto"
}
